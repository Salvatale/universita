A = [1 1/2 1/3; 1/2 1/3 1/4; 1/3 1/4 1/5]

A =

       1              1/2            1/3     
       1/2            1/3            1/4     
       1/3            1/4            1/5     

b = [11/6 13/12 47/60]'

b =

      11/6     
      13/12    
      47/60    

x = A\b

x =

       1       
       1       
       1       

err = abs(ones(3,1)-x)

err =

       1/450359962737050
       1/76332197074076
       1/80421421917330

format long e
x = A\b

x =

     1.000000000000002e+00
     9.999999999999869e-01
     1.000000000000012e+00

err = abs(ones(3,1)-x)

err =

     2.220446049250313e-15
     1.310063169057685e-14
     1.243449787580175e-14

inv(A)

